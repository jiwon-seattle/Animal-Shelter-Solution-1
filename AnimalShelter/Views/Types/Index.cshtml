@{
    Layout = "_Layout";
}
@using AnimalShelter.Models;
@model List<AnimalShelter.Models.Types>;
<h1>Types:</h1>

@if (Model.Count == 0)
{
    <h3>No types have been added yet.</h3>
}
<ul>
    @foreach (Types types in Model)
    {
        <li class="link">@Html.ActionLink($"{types.Name}", "Details", new {id = types.TypesId})</li>
    }
</ul>

<p class="link">@Html.ActionLink("Add new types", "Create")</p>
<p class="link">@Html.ActionLink("Home", "Index", "Home")</p>